<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="#FFF25788"></solid>
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp"></corners>
            <stroke android:width="1dp" android:color="#FFF25788"></stroke>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape>
            <solid android:color="#ffffff"></solid>
            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp"></corners>
            <stroke android:width="1dp" android:color="#FFF25788"></stroke>
        </shape>
    </item>
</selector>